{

  "commands": [
    {
      "id": "start",
      "test": ["\/start"],
      "handler": "command-start"
    },
    {
      "id": "stop",
      "test": ["\/stop"],
      "handler": "command-stop"
    },
    {
      "id": "ping",
      "test": ["\/ping", "\/fixed"],
      "handler": "command-ping"
    },
    {
      "id": "ok",
      "test": ["\/ok"],
      "handler": "command-ok"
    },
    {
      "id": "not_ok",
      "test": ["\/!ok"],
      "handler": "command-not-ok"
    },
    {
      "id": "busy",
      "test": ["\/busy"],
      "handler": "command-busy"
    },
    {
      "id": "add",
      "test": ["\/add (@?\\w+)"],
      "handler": "command-add"
    },
    {
      "id": "change",
      "test": ["\/change (@?\\w+) to (@?\\w+)"],
      "handler": "command-change"
    },
    {
      "id": "remove",
      "test": ["\/remove (@?\\w+)"],
      "handler": "command-remove"
    },
    {
      "id": "replace",
      "test": ["\/replace (@?\\w+)"],
      "handler": "command-replace"
    },
    {
      "id": "help",
      "test": ["\/help"],
      "handler": "command-help"
    }
  ]

}
