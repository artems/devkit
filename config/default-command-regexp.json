{

  "commands": [
    {
      "test": "\/start",
      "handlers": ["command-start"]
    },
    {
      "test": "\/stop",
      "handlers": ["command-stop"]
    },
    {
      "test": "\/ping",
      "handlers": ["command-ping"]
    },
    {
      "test": "\/fixed",
      "handlers": ["command-ping"]
    },
    {
      "test": "\/ok",
      "handlers": ["command-ok"]
    },
    {
      "test": "\/!ok",
      "handlers": ["command-not-ok"]
    },
    {
      "test": "\/busy",
      "handlers": ["command-busy"]
    },
    {
      "test": "^занят",
      "handlers": ["command-busy"]
    },
    {
      "test": "\/add (@\\w+)",
      "handlers": ["command-add"]
    },
    {
      "test": "\/change (@\\w+) to (@\\w+)",
      "handlers": ["command-change"]
    },
    {
      "test": "\/remove (@\\w+)",
      "handlers": ["command-remove"]
    },
    {
      "test": "\/replace (@\\w+)",
      "handlers": ["command-replace"]
    },
    {
      "test": "\/help",
      "handlers": ["command-help"]
    }
  ]

}
