{
  "name": "devexp",
  "author": [
    "Artem Semenov <tema.semenov@gmail.com>",
    "Stanislav Sysoev <d4rkr00t@gmail.com> (http://sysoev.org)"
  ],
  "license": "MIT",
  "version": "0.2.0",
  "description": "Developer Experiments",
  "repository": {
    "url": "https://github.com/devexp-org/devexp"
  },
  "main": "start.js",
  "pre-commit": [
    "eslint",
    "test"
  ],
  "pre-commit.silent": true,
  "scripts": {
    "test": "$npm_package_config_mocha $npm_package_config_pattern_test",
    "test:e2e": "$npm_package_config_mocha $npm_package_config_pattern_e2e",
    "test:watch": "$npm_package_config_mocha --watch --reporter spec $npm_package_config_pattern_test",
    "start": "node start.js",
    "start:watch": "nodemon start.js -e js,json,html --ignore node_modules/",
    "jsdoc": "jsdoc -r -d jsdoc src",
    "eslint": "eslint .",
    "coverage": "$npm_package_config_istanbul $npm_package_config_pattern_test",
    "coverage:e2e": "$npm_package_config_istanbul $npm_package_config_pattern_test $npm_package_config_pattern_e2e",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "badgs": "^1.0.0",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "github": "^0.2.4",
    "intel": "^1.1.0",
    "lodash": "^4.6.1",
    "minimatch": "^3.0.0",
    "moment": "^2.10.6",
    "mongoose": "^4.4.7",
    "node-xmpp-client": "3.0.0",
    "nodemailer": "^2.3.1",
    "response-time": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "eslint": "2.6.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.3.4",
    "node-architect": "^0.0.8",
    "nodemon": "^1.8.1",
    "pre-commit": "^1.1.2",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2",
    "supertest": "^1.2.0"
  },
  "config": {
    "mocha": "mocha --compilers js:babel-register -r ./tests/setup.js --reporter progress",
    "istanbul": "babel-node ./node_modules/.bin/istanbul cover --report text --report lcovonly --report html _mocha -- -r ./tests/setup.js --reporter dot",
    "pattern_e2e": "./tests/e2e/**.js",
    "pattern_test": "./src/**/__tests__/**.js"
  }
}
