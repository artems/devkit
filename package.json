    "karma": "^0.13.3",
    "karma-coverage": "douglasduteil/karma-coverage#next",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.8",
    "pre-commit": "^1.0.10",
    "sinon": "^1.15.4"
  }
